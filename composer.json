{
  "name": "idapgroup/s3-storage-sdk",
  "type": "library",
  "description": "A PHP wrapper for S3 Storage",
  "homepage": "https://github.com/idapgroup/S3-Storage-SDK",
  "keywords": [
    "S3",
    "s3",
    "storage"
  ],
  "license": "BSD-3-Clause",
  "version": "0.1.0",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Vlad Chyzhevych",
      "email": "vlad.chyzhevych@idapgroup.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "aws/aws-sdk-php": "^3.147"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.3"
  },
  "autoload": {
    "psr-4": {
